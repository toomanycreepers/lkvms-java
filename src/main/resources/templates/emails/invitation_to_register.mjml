<mjml>
  <mj-head>
    <mj-title>Эликвис</mj-title>
    <mj-preview>Эликвис</mj-preview>
    <mj-attributes>
      <mj-all
        font-family="Verdana, Helvetica, Arial, sans-serif"
        padding="0px"
      ></mj-all>
      <mj-text
        font-weight="400"
        font-size="16px"
        color="#76004B"
        line-height="135%"
      ></mj-text>
    </mj-attributes>
  </mj-head>
  <mj-body width="600px" background-color="#1C0F81">
    <mj-section background-color="#fff" padding="15px 0 60px 0">
      <mj-column padding="0 16px" width="532px">
        <mj-image
          padding="0"
          width="248px"
          align="center"
          src="https://i.imgur.com/5lS7Jvo.png"
        />
      </mj-column>
    </mj-section>
    <mj-section background-color="#fff">
      <mj-column padding="0 16px" width="500px">
        <mj-text
          padding="0 0 20px 0"
          align="center"
          font-family="Verdana"
          font-weight="700"
          font-size="20px"
          line-height="120%"
          color="#1C0F81"
        >
          Здравствуйте!
        </mj-text>
        <mj-text
          padding="0 0 30px 0"
          align="center"
          font-family="Verdana"
          font-weight="700"
          font-size="32px"
          line-height="120%"
